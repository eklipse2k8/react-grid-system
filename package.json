  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eklipse2k8/react-grid-system/issues"
  },
  "homepage": "https://github.com/eklipse2k8/react-grid-system/issues",
  "resolutions": {
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7"
  },
  "peerDependencies": {
    "@stitches/react": ">=1.2.8",
    "@types/react": "*",
