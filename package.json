    "lint": "eslint --ignore-path .gitignore --fix *.ts src/**/*.ts",
    "prepare": "yarn build"
  },
  "type": "module",
  "types": "./types/index.d.ts",
  "files": [
    "build",
    "src",
    "index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/eklipse2k8/react-grid-system.git"
  },
  "keywords": [
