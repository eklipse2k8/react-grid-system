{
  "name": "@eklipse2k8/react-grid-system",
  "version": "8.1.6",
  "description": "A powerful Bootstrap-like responsive grid system for React.",
  "main": "./build/index.js",
  "scripts": {
    "start": "styleguidist server",
    "clean": "rimraf build && rimraf types",
    "build": "swc src --out-dir build --extensions \".ts,.tsx\" --source-maps inline && tsc",
    "docs": "styleguidist build",
    "lint": "eslint --ignore-path .gitignore --fix *.ts src/**/*.ts",
    "prepare": "yarn build"
  },
  "type": "module",
  "types": "./types/index.d.ts",
  "files": [
    "build",
    "src",
    "index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/eklipse2k8/react-grid-system.git"
  },
  "keywords": [
    "react",
    "bootstrap",
    "grid",
    "responsive"
  ],
  "author": "Sealninja",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eklipse2k8/react-grid-system/issues"
  },
  "homepage": "https://github.com/eklipse2k8/react-grid-system/issues",
  "peerDependencies": {
    "@stitches/react": ">=1.2.8",
    "react": "^16.8.0 || ^17.x || ^18.x"
  },
  "dependencies": {
    "prop-types": "^15.8.1"
  },
  "devDependencies": {
    "@stitches/react": "^1.2.8",
    "@swc/cli": "^0.1.59",
    "@swc/core": "^1.3.25",
    "eslint": "^8.31.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "prettier": "^2.8.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.4",
    "yarn-deduplicate": "^6.0.1"
  }
}
